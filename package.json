{
  "name": "1.-effective-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --inspect dist/main.js",
    "watch": "tsc -w",
    "build": "tsc",
    "start": "tsc && node dist/main.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "format": "prettier \"src/**/*.ts\" ",
    "format:fix": "prettier --write \"src/**/*.ts\" ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check:unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "check:packages": "npx npm-check-updates -f '/^(?!.*eslint)/' --format group",
    "packages:fix": "ncu -u -f '/^(?!.*eslint)/' && npm i",
    "check:depcheck": "npx depcheck --ignores \"@types/multer\"",
    "dev:fix": "npm run lint:fix && npm run format:fix && npm run packages:fix",
    "pre-commit": "npm run lint && npm run format && npm run check:unused-exports && npm run check:packages && npm run check:depcheck"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
